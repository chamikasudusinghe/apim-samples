          <activiti:value id="REJECTED" name="REJECTED"></activiti:value>
        </activiti:formProperty>
        <modeler:group-info-name-workflow xmlns:modeler="http://activiti.com/modeler"><![CDATA[workflow]]></modeler:group-info-name-workflow>
